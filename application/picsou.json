{
    "title": "PICSOU en action",
    "info": "Analyse les cours de la bourse et propose des ordres d'achat et de vente de valeurs",
    "menu_orientation": "horizontal",
    "icon_file": "picsou.jpg",
    "basename": "/mnt/nas/data/picsou/picsou.sqlite",
    "basename1": "/mnt/nas/data/picsou/png/2020-04-02/picsou.sqlite",
    "basename2": "/mnt/nas/data/picsou/png/2020-04-03/picsou.sqlite",
    "basehost": "~/data/picsou/picsou.sqlite",
    "data_directory": "/mnt/nas/data/picsou",
    "trade": {
        "stake": 1200,
        "cost": 0.0048,
        "macdbuy": 40,
        "rsimin": 20,
        "rsimax": 60
    },
    "constants": {
        "cost": 0.0048
    },
    "scheduler": {
        "batch": "./picsou_batch.py -day",
        "periode_mn": 10
    },
    "shell": {
        "Picsou Backup": "./picsou_batch.py -backup",
        "Picsou Restauration": "./picsou_batch.py -restore",
        "Picsou Trade": "./picsou.py -day -trade",
        "Picsou Quote": "./picsou.py -day -quote",
        "Picsou Repeat": "./picsou.py -dayrepeat -quote -trade"
    },
    "tables": {
        "ptf": {
            "key": "ptf_id",
            "key_display": "ptf_name",
            "elements": {
                "ptf_id": {
                    "label_long": "Valeur",
                    "label_short": "Valeur",
                    "type": "text",
                    "sortable": true,
                    "searchable": true
                },
                "ptf_name": {
                    "label_long": "Nom",
                    "label_short": "Nom",
                    "type": "text",
                    "searchable": true,
                    "sortable": true,
                    "col_width": 140
                },
                "ptf_note": {
                    "label_long": "Note",
                    "label_short": "Note",
                    "type": "text",
                    "col_editable": true,
                    "searchable": true
                },
                "ptf_enabled": {
                    "label_long": "Valeur Active",
                    "label_short": "Active",
                    "type": "check",
                    "col_editable": true
                },
                "ptf_top": {
                    "label_long": "TOP",
                    "label_short": "TOP",
                    "type": "check",
                    "sortable": true,
                    "col_editable": true
                },
                "ptf_gain": {
                    "label_long": "Gain",
                    "label_short": "Gain",
                    "type": "float",
                    "display": "%3.2f €",
                    "sortable": true,
                    "sql_color": "case when ptf_gain < 0 then '#FF0000' else '#006600' end"
                },
                "_yahoo": {
                    "label_long": "Lien vers Yahoo",
                    "label_short": "Yahoo",
                    "type": "url",
                    "params": {
                        "url": "https://fr.finance.yahoo.com/chart/{ptf_id}",
                        "icon_name": "applications-internet"
                    }
                },
                "date_du_jour": {
                    "label_long": "Date",
                    "label_short": "Date",
                    "type": "text",
                    "jointure": {
                        "join": "left outer join (select date('now') as date_du_jour) as t",
                        "column": "date_du_jour"
                    }
                },
                "_graph": {
                    "label_long": "Graph",
                    "label_short": "Graph",
                    "type": "plugin",
                    "params": {
                        "plugin": "picsou_graph.PicsouGraphDay",
                        "icon_name": "applications-office"
                    },
                    "args": {
                        "ptf_id": "{ptf_id}",
                        "path": "png/{date_du_jour}/{ptf_id}.png"
                    }
                },
                "_image": {
                    "label_long": "Graph",
                    "label_short": "Graph",
                    "type": "plugin",
                    "params": {
                        "plugin": "picsou_image.PicsouImage",
                        "icon_name": "insert-image"
                    },
                    "args": {
                        "path": "png/{date_du_jour}/{ptf_id}.png"
                    }
                },
                "_batch": {
                    "label_long": "Actualiation des données",
                    "label_short": "Actualiser...",
                    "type": "batch",
                    "params": {
                        "plugin": "picsou_batchui.PicsouBatchUi",
                        "icon_name": "view-refresh"
                    }
                },
                "_vptf": {
                    "label_long": "Historique",
                    "label_short": "Historique",
                    "type": "view",
                    "height": 300,
                    "params": {
                        "table": "trades",
                        "view": "vptf"
                    },
                    "args": {
                        "trades_ptf_id": "{ptf_id}"
                    }
                },
                "_backup": {
                    "label_long": "Backup des données",
                    "label_short": "Backup...",
                    "type": "batch",
                    "params": {
                        "plugin": "picsou_backup.PicsouBackup",
                        "icon_name": "view-refresh"
                    }
                }
            },
            "views": {
                "vactif": {
                    "title": "Valeurs Actives",
                    "form_add": "fadd",
                    "form_edit": "fedit",
                    "icon_name": "folder",
                    "searchable": true,
                    "elements": {
                        "ptf_id": {},
                        "ptf_name": {},
                        "ptf_enabled": {},
                        "date_du_jour": {},
                        "_graph": {},
                        "ptf_top": {},
                        "ptf_gain": {},
                        "ptf_note": {},
                        "_yahoo": {}
                    },
                    "sql_where": "ptf_enabled = '1'",
                    "order_by": "ptf_gain desc, ptf_id",
                    "sql_footer": "SELECT printf('PICSOU total : <b>%.2f</b> €', sum(ptf_gain)) AS sql_footer FROM ptf where ptf_top = 1"
                },
                "vall": {
                    "title": "Toutes les valeurs",
                    "form_add": "fadd",
                    "form_edit": "fedit",
                    "icon_name": "folder",
                    "deletable": true,
                    "searchable": true,
                    "elements": {
                        "ptf_id": {},
                        "ptf_name": {},
                        "ptf_enabled": {},
                        "date_du_jour": {},
                        "_graph": {},
                        "ptf_gain": {},
                        "ptf_note": {},
                        "_yahoo": {}
                    },
                    "order_by": "ptf_id"
                },
                "vsimul": {
                    "info1": "Vue particulière lus par les plugins batch",
                    "info2": "todo: enlever cette contrainte picsou.py ligne 142",
                    "title": "Simulateur",
                    "hide": true,
                    "elements": {
                        "ptf_id": {},
                        "_batch": {}
                    }
                }
            },
            "forms": {
                "fadd": {
                    "title": "Ajout d'un titre",
                    "elements": {
                        "ptf_id": {},
                        "ptf_name": {},
                        "ptf_enabled": {},
                        "ptf_note": {}
                    }
                },
                "fedit": {
                    "title": "Mise à jour d'un titre",
                    "elements": {
                        "ptf_id": {},
                        "ptf_name": {},
                        "ptf_enabled": {},
                        "_yahoo": {},
                        "ptf_note": {},
                        "_vptf": {}
                    }
                }
            }
        },
        "orders": {
            "key": "orders_id",
            "key_display": "orders_id",
            "elements": {
                "orders_id": {
                    "label_long": "id",
                    "label_short": "id",
                    "type": "counter",
                    "hide": false
                },
                "orders_ptf_id": {
                    "label_long": "Valeur",
                    "label_short": "Valeur",
                    "type": "combo",
                    "sql_items": "SELECT distinct quotes.id, quotes.name FROM quotes",
                    "searchable": true,
                    "sortable": true
                },
                "_ptf_name": {
                    "label_long": "Nom de la valeur",
                    "label_short": "Nom",
                    "type": "text",
                    "searchable": true,
                    "sortable": true,
                    "jointure": {
                        "join": "LEFT OUTER JOIN ptf ON ptf_id = orders_ptf_id",
                        "column": "ptf_name"
                    }
                },
                "orders_order": {
                    "label_long": "Ordre",
                    "label_short": "Ordre",
                    "sortable": true,
                    "searchable": true,
                    "type": "combo",
                    "col_align": "center",
                    "items": {"buy":"Achat","sell":"Vente"}
                },
                "orders_time": {
                    "label_long": "Jour Heure",
                    "label_short": "Jour Heure",
                    "type": "text",
                    "sortable": true,
                    "searchable": true,
                    "default_sql": "select datetime('now', 'localtime')"
                },
                "orders_quote": {
                    "label_long": "Cours actuel",
                    "label_short": "Cours actuel",
                    "type": "float",
                    "display": "%3.2f €",
                    "sql_text": "select close from quotes where id = '{orders_ptf_id}' and date = (select max(date) from quotes where id = '{orders_ptf_id}')",
                    "jointurexx": {
                        "join": "left outer join (select qq.id, close from quotes as qq join (select id, max(date) as datemax from quotes group by id) as dd on dd.id = qq.id and datemax = qq.date) as tt on tt.id = orders_ptf_id",
                        "column": "close"
                    }
                },
                "orders_quantity": {
                    "label_long": "Quantité",
                    "label_short": "Quantité",
                    "col_align": "center",
                    "type": "int"
                },
                "orders_quoter": {
                    "label_long": "Cours d'exécution",
                    "label_short": "Cours exec",
                    "type": "float",
                    "display": "%3.2f €"
                },
                "orders_cost_price": {
                    "label_long": "Prix de revient",
                    "label_short": "Revient à",
                    "type": "float",
                    "display": "%3.2f €",
                    "col_width": 80,
                    "sql_put": "({orders_quoter} * {orders_quantity} + {orders_quoter} * {orders_quantity} * ({__cost}*2))/{orders_quantity}"
                },
                "orders_cost": {
                    "label_long": "Frais",
                    "label_short": "Frais",
                    "type": "float",
                    "display": "%3.2f €",
                    "col_width": 60,
                    "sql_put": "{orders_quoter} * {orders_quantity} * {__cost}"
                },
                "orders_amount": {
                    "label_long": "Mvt Banque",
                    "label_short": "Mont. TTC",
                    "type": "float",
                    "display": "%3.2f €",
                    "col_width": 80,
                    "sql_put": "{orders_quoter} * {orders_quantity} + {orders_quoter} * {orders_quantity} * {__cost}"
                },
                "orders_gain": {
                    "label_long": "Gain",
                    "label_short": "Gain",
                    "type": "float",
                    "col_width": 80,
                    "display": "%3.2f €",
                    "sql_put": "({orders_quote} * {orders_quantity}) - ({orders_quoter} * {orders_quantity} - {orders_quoter} * {orders_quantity} * {__cost})",
                    "sql_color": "case when orders_gain < 0 then '#FF0000' else '#006600' end"
                },
                "orders_gainp": {
                    "label_long": "Gain en %",
                    "label_short": "en %",
                    "type": "float",
                    "col_width": 80,
                    "display": "%3.2f %%",
                    "sql_put": "((({orders_quote} * {orders_quantity}) - ({orders_quoter} * {orders_quantity} - {orders_quoter} * {orders_quantity} * {__cost})) / ({orders_quoter} * {orders_quantity}))*100",
                    "sql_color": "case when orders_gainp < 0 then '#FF0000' else '#006600' end"
                },
                "_fsell": {
                    "label_long": "Vendre",
                    "label_short": "Vendre",
                    "type": "form",
                    "params": {
                        "form": "fsell",
                        "icon_name": "media-playback-stop"
                    },
                    "args": {
                    }
                }
            },
            "views": {
                "vbuy": {
                    "title": "Achat",
                    "form_add": "faddbuy",
                    "form_edit": "feditbuy",
                    "searchable": true,
                    "deletable": true,
                    "icon_name": "go-up",
                    "limit": 100,
                    "elements": {
                        "orders_id": {"hide":true},
                        "orders_ptf_id": {},
                        "_ptf_name": {},
                        "orders_quote": {},
                        "orders_time": {},
                        "orders_order": {},
                        "orders_quoter": {},
                        "orders_cost_price": {},
                        "orders_quantity": {},
                        "orders_cost": {},
                        "orders_amount": {},
                        "orders_gain": {},
                        "orders_gainp": {},
                        "_fsell": {}
                    },
                    "sql_where": "orders.orders_order = 'buy'",
                    "order_by": "orders.orders_time desc"
                },
                "vsell": {
                    "title": "Vente",
                    "form_edit": "feditsell",
                    "searchable": true,
                    "deletable": true,
                    "icon_name": "go-down",
                    "limit": 100,
                    "elements": {
                        "orders_id": {"hide":true},
                        "orders_ptf_id": {},
                        "_ptf_name": {},
                        "orders_quote": {},
                        "orders_time": {},
                        "orders_order": {},
                        "orders_quoter": {},
                        "orders_cost_price": {},
                        "orders_quantity": {},
                        "orders_cost": {},
                        "orders_amount": {},
                        "orders_gain": {},
                        "orders_gainp": {}
                    },
                    "sql_where": "orders.orders_order = 'sell'",
                    "order_by": "orders.orders_time desc"
                },
                "vptf": {
                    "title": "Historique",
                    "form_add": "fadd",
                    "form_edit": "fedit",
                    "searchable": true,
                    "deletable": true,
                    "icon_name": "go-jump",
                    "limit": 100,
                    "hide": true,
                    "elements": {
                        "orders_id": {"hide":true},
                        "orders_ptf_id": {"hide":true},
                        "_ptf_name": {},
                        "orders_time": {},
                        "orders_order": {},
                        "orders_quote": {},
                        "orders_quantity": {},
                        "orders_cost": {}
                    },
                    "order_by": "orders.orders_time desc"
                }
            },
            "forms": {
                "faddbuy": {
                    "title": "Ajout d'un ordre d'Achat",
                    "elements": {
                        "orders_id": {"hide":true},
                        "orders_ptf_id": {},
                        "orders_order": {"protected": true, "default": "buy"},
                        "orders_time": {}
                    }
                },
                "feditbuy": {
                    "title": "Mise à jour d'un ordre d'Achat",
                    "elements": {
                        "orders_id": {"hide":true},
